{
    "extends": "airbnb",
    "rules": {
        "react/prefer-stateless-function": [0],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": [0],
        "class-methods-use-this": [0]
    },
    "settings": {
      "import/resolver": "reactnative"
    }
}